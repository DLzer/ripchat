{
    "name": "dlzer/ripchat",
    "description": "A timed, secure chat framework",
    "require": {
        "illuminate/database": "^8.0",
        "monolog/monolog": "^2.1",
        "php-di/php-di": "^6.0",
        "predis/predis": "^1.1",
        "selective/config": "^0.1.1",
        "slim/http": "^1.0",
        "slim/psr7": "^1.0",
        "slim/slim": "4.*",
        "vlucas/phpdotenv": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
